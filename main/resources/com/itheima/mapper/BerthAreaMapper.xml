<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.BerthAreaMapper">
    <insert id="addRecord">
        insert into bertharea(id,iban,size,damaged,repairs_cost)
        values (#{id},#{iban},#{size},#{damaged},#{repairs_cost})
    </insert>
</mapper>